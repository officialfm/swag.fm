{
  "name": "Swag.fm",
  "version": "0.3",
  "description": "Add official.fm tracks and playlists to your swag.fm page",
  "background": { "scripts": [
    "javascript/chrome_ex_oauth.js",
    "javascript/chrome_ex_oauthsimple.js",
    "javascript/jquery-1.7.2.min.js",
    "javascript/client.js",
    "javascript/settings.js",
    "javascript/background.js"
  ]},
  "web_accessible_resources": ["chrome_ex_oauth.html"],
  "page_action": {
    "default_icon": "icon.ico",
    "default_title": "Add to your swag.fm page!"
  },
  "update_url": "http://swag.fm/extensions/chrome.xml",
  "permissions": ["tabs", "http://0.0.0.0:3000/" ],
  "manifest_version": 2
}
